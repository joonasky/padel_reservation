{
  "name": "padel-reservation",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "migrate": "npx dotenv -e .env.local -- npx prisma migrate dev",
    "dev": "npx dotenv -e .env.local -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:setup": "docker-compose -f docker-compose.tests.yml up -d && sleep 3 && dotenv -e .env.test -- npx prisma migrate deploy",
    "test:e2e": "dotenv -e .env.test -- playwright test ./tests/e2e/",
    "test:teardown": "docker-compose -f docker-compose.tests.yml down",
    "test:unit": "jest"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/types": "^7.2.14",
    "@mui/x-date-pickers": "^7.2.0",
    "@prisma/client": "^4.5.0",
    "babel-jest": "^29.3.1",
    "babel-plugin-superjson-next": "^0.4.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "identity-obj-proxy": "^3.0.0",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "recharts": "^2.1.15"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "dotenv-cli": "^7.4.1",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "jest-fetch-mock": "^3.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next-router-mock": "^0.8.0",
    "prisma": "^4.5.0",
    "typescript": "latest"
  }
}
